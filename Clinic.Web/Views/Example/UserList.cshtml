@model Clinic.ViewModel.User.UserListViewModel
@{
    ViewData["Title"] = "Home Page";
}

<br/>
<div class="container">
    <div class="row">
        <table class="table table-bordered">
            <tr><th>Id</th><th>PIB</th></tr>
            @foreach (var user in Model.Users)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.PIB</td>
                </tr>
            }
        </table>
    </div>

</div>
